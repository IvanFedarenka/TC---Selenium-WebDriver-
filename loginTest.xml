<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="LoginSuite" verbose="1">
    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>
    <test name="Log In">
        <classes>
            <class name="tests.LoginTest">
                <methods>
                    <include name="testLogin"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Log Out">
        <classes>
            <class name="tests.LoginTest">
                <methods>
                    <include name="testLogout"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>